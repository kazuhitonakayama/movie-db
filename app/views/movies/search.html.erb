<h1>映画検索</h1>

<div>
  <%= form_tag(movies_search_path, method: :get) do %>
    <%= search_field_tag :looking_for, nil, placeholder: 'Movie Title...' %>
    <%= submit_tag '検索' %>
  <% end %>
</div>


<% @movies.each do |movie| %>
    <script>
        for (let i = 0, l = movie.count; i < l; i++) {
            number = puts i;
            <%= movie['table']['results'][number]['table']['title'] %>
        }
    </script>
<% end %>
